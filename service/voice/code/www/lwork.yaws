<script language="javascript">
    function getVoipWebSocketURL(){
      return 'ws://' + $('#lw_ws_origin').text() + '/websockets_endpoint.yaws';
    }
    function getVideoWebSocketURL(){
      return 'ws://' + $('#lw_ws_origin').text() + '/vs_agent.yaws';
