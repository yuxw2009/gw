//%	alert("2222222222222222");
        var datameetingController = DataMeetingController.createNew();
//%	alert("3333333333333333");
//%        var datameeting_ua = Datameeting_UA.createNew();
        meetingController.init($('#meeting'));
        datameetingController.init($('#datameeting'));
        loadContent.init();
    function setHeight() {
       var clientHeight = document.body.clientHeight;
       $('#article').css('min-height', clientHeight+110+'px');
       $('.contianer_contact').css('height', clientHeight - 150 +'px');
       $('#contact').css('height', clientHeight -150 +'px');
       return false;
    };
    setHeight(); 
    var $backToTopTxt = "", $backToTopEle = $('<div id="goto_top"/>').appendTo($("body"))
      .attr("title", $backToTopTxt).click(function() {
      $("html, body").animate({ scrollTop: 0 }, 120);
    }), $backToTopFun = function() {
      var st = $(document).scrollTop(), winh = $(window).height();
      (st > 0)? $backToTopEle.show(): $backToTopEle.hide();    
      //IE6下的定位
      if (!window.XMLHttpRequest) {
        $backToTopEle.css("top", st + winh - 166);    
      }
    };    
      $backToTopFun();
        //右侧栏目浮动     
      $(window).scroll(function () {
            $backToTopFun();
            return false;
      });
      $('#modify_images').find('form').ajaxForm({ 
         complete: function(xhr) {         
          var data = JSON.parse(xhr['responseText']);
             if(data['status'] === 'ok'){
               return loadContent.updateimg.updateimages(data['file_name']);
             }else{
                LWORK.msgbox.show("上传失败！", 1, 2000); 
                return false;
             }
        }

    });
     
    $('#upload_filecontent').find('form').ajaxForm({    
      beforeSend: function() {
        $('#upload_filecontent').find('.progress_'+ form_index).show();  
        var percentVal = '0%';
        $('#upload_filecontent').find('.bar_'+ form_index).width(percentVal)
        $('#upload_filecontent').find('.percent_'+ form_index).html(percentVal);    
      },
      uploadProgress: function(event, position, total, percentComplete) {
        var percentVal = percentComplete + '%';
        $('#upload_filecontent').find('.bar_'+ form_index).width(percentVal)
        $('#upload_filecontent').find('.percent_'+ form_index).html(percentVal);
      },
      complete: function(xhr) {
       var data = JSON.parse(xhr['responseText']) ;
       if(data['status'] === 'failed'){
         $('#upload_filecontent').find('.progressBarStatus_'+ form_index).html('<p class="upload_error">上传失败</p>');             
       }else{
         $('#upload_filecontent').find('.progressBarStatus_'+ form_index).html('<p class="upload_success">上传成功</p>');    
         opt = {uuid:uuid, file_name:data['name'], file_id:(data['doc_id']).toString(), file_size:data['length'].toString(), content:'', members:[], 't':new Date().getTime()}; 
          loadContent.publish('documents', opt, $('#file_share_container'));       
        }
       }
      }); 
    
      var form_index = -1;
      function sharefiles_handle(obj){
        var con = $("#upload_filecontent");
        var _this = $(obj);
        var $val = _this.val(),
         valArray = $val.split('\\');
         filename = valArray[valArray.length - 1];
         form_index = form_index + 1 ;
         var html = ['<div class="progressContainer blue"><a class="progressCancel" href="#" style="display: none;"> </a>',
         '<div class="progressName">'+ filename +'</div>',
         '<div class="progressBarStatus progressBarStatus_'+ form_index +'">',
         '<div class="progress progress_'+ form_index +'">',
         '<div class="bar bar_'+ form_index +'"></div >',
         '<div class="percent percent_'+ form_index +'">0%</div >',
         '</div>',
         '</div>',
         '<div class="progressBarComplete" style=""></div>',
         '</div>'].join(""); 
         con.find('.upload_main').append(html);
         con.find('.submit_sharefile').click();
    } 
    /*集体决策传图片*/  
     $('#polls_option').find('form').ajaxForm({
         dataType:  'json', 
         complete: function(xhr) {
          var data = JSON.parse(xhr['responseText']);
          if(data['status'] === 'ok'){
            var url = loadContent.getpicphoto(data['file_name'], 'S');
            $('#polls_option').find('.current_item img').removeClass('loading_img').addClass('display_img').attr({'src':url, 'source': data['file_name'] });
            $('#polls_option').find('.current_item').removeClass('current_item');
          }else{
             LWORK.msgbox.show("上传失败！", 1, 2000); 
          }
       }
    }); 

    function upload_itemimages(obj){
      var _this = $(obj); 
      var $val = _this.val();
      var valArray = $val.split('\\');
      var filename = valArray[valArray.length - 1];
      var filetype = loadContent.filetype_handle(filename);
          filetype = filetype.replace(/(^\s*)|(\s*$)/g, "");
      if(filetype !== 'jpg' && filetype !== 'png'&& filetype !== 'jpeg' && filetype !== 'gif'&& filetype !== 'bmp'){
           LWORK.msgbox.show("请上传不超过1M图片文件！", 2, 1500); 
           return false;
      }        
      var index =  $('#polls_option').find('li').index(_this.parent().parent().parent()); 
      _this.next().find('img').removeClass('display_img').addClass('loading_img').attr('src', '/images/uploading.gif');   
      _this.parent().find('input[type=submit]').click();
      _this.parent().addClass('current_item');
    }
    window.onbeforeunload = function () {
      if($('.Interrupt').css('display') !=='block'){
          return "该操作将会退出Lwork工作平台吗？";
      }
    }
  </script>
